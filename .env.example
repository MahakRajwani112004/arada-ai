# Magure AI Platform - Environment Configuration

# Application
APP_ENV=development
APP_DEBUG=true
APP_NAME=magure-ai-platform

# Temporal
TEMPORAL_HOST=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=agent-tasks

# PostgreSQL
DATABASE_URL=postgresql+asyncpg://magure:magure_dev_password@localhost:5432/magure_db

# Redis
REDIS_URL=redis://localhost:6379/0

# Qdrant Vector DB
QDRANT_HOST=localhost
QDRANT_PORT=6333

# LLM Providers
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Security
SECRET_KEY=your-secret-key-change-in-production

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
